#include <iostream>
using namespace std;

int main() {
	setlocale(0, "");
	cout << "Введи число от 1 до 100, смертный: ";
	int num;
	cin >> num;
	if (num < 0 or num>100) {
		cout << "нельзя";
	}
	else if (num >= 1 and num <= 10) {
		cout << "в диапозоне между 1 и 10";
	}
	else if (num >= 11 and num <= 20) {
		cout << "в диапозоне между 11 и 20";
	}
	else if (num >= 21 and num <= 30) {
		cout << "в диапозоне между 21 и 30";
	}
	else if (num >= 31 and num <= 40) {
		cout << "в диапозоне между 31 и 40";
	}
	else if (num >= 41 and num <= 50) {
		cout << "в диапозоне между 41 и 50";
	}
	else if (num >= 51 and num <= 60) {
		cout << "в диапозоне между 51 и 60";
	}
	else if (num >= 61 and num <= 70) {
		cout << "В диапозоне между 61 и 70";
	}
	else if (num >= 71 and num <= 80) {
		cout << "В диапозоне между 71 и 80";
	}
	else if (num >= 81 and num <= 90) {
		cout << "В диапозоне между 81 и 90";
	}
	else if (num >= 91 and num <= 100) {
		cout << "В диапозоне между 91 и 100";
	}
	return 0;
}
